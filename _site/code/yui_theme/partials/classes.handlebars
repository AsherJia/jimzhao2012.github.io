<h1>{{name}} Class</h1>
<div class="box meta">
    {{#if uses}}
        <div class="uses">
            Uses
            <ul class="inline commas">
                {{#uses}}
                    <li><a href="{{.}}.html">{{.}}</a></li>
                {{/uses}}
            </ul>
        </div>
    {{/if}}

    {{#if extends}}
        <div class="extends">
            扩展 {{#crossLink extends}}{{/crossLink}}
        </div>
    {{/if}}

    {{#if foundAt}}
        <div class="foundat">
            定义在: <a href="{{foundAt}}">`{{{file}}}:{{{line}}}`</a>
        </div>
    {{/if}}

    {{#if module}}
        {{#if submodule}}
            模块: {{#crossLinkModule submodule}}{{/crossLinkModule}}<br>
            父模块: {{#crossLinkModule module}}{{/crossLinkModule}}
        {{else}}
            模块: {{#crossLinkModule module}}{{/crossLinkModule}}
        {{/if}}
    {{/if}}

    {{#if since}}
        <p>可用版本 {{since}}</p>
    {{/if}}
</div>

{{#if deprecated}}
    <div class="box deprecated">
        <p>
        {{#if deprecationMessage}}
            <strong>弃用:</strong> {{deprecationMessage}}
        {{else}}
            This class is deprecated.
        {{/if}}
        </p>
    </div>
{{/if}}

<div class="box intro">
    {{{classDescription}}}
</div>

{{#is_constructor}}
    <div class="constructor">
        <h2>构造函数</h2>
        {{>method}}
    </div>
{{/is_constructor}}

<div id="classdocs" class="tabview">
    <ul class="api-class-tabs">
        <li class="api-class-tab index"><a href="#index">索引</a></li>

        {{#if methods}}
            <li class="api-class-tab methods"><a href="#methods">方法</a></li>
        {{/if}}
        {{#if properties}}
            <li class="api-class-tab properties"><a href="#properties">成员</a></li>
        {{/if}}
        {{#if attrs}}
            <li class="api-class-tab attrs"><a href="#attrs">属性</a></li>
        {{/if}}
        {{#if events}}
            <li class="api-class-tab events"><a href="#events">事件</a></li>
        {{/if}}
    </ul>

    <div>
        <div id="index" class="api-class-tabpanel index">
            <h2 class="off-left">条目索引</h2>

            {{#if methods}}
                <div class="index-section methods">
                    <h3>方法</h3>
                    <br/>

                    <table>
                    <tr><th>描述</th><th>函数</th></tr>
                    {{#methods}}
                    <tr><td>{{brief}}</td><td><a href="#method_{{name}}">{{name}}</a></td></tr>
                    {{/methods}}              
                    </table>


                    <ul class="index-list methods{{#if extends}} extends{{/if}}">
                        {{#methods}}
                            <li class="index-item method{{#if access}} {{access}}{{/if}}{{#if deprecated}} deprecated{{/if}}{{#if overwritten_from}} inherited{{/if}}{{#if extended_from}} inherited{{/if}}">

                                {{#if static}}
                                    <span class="flag static">static</span>
                                {{/if}}
                                {{#if deprecated}}
                                    <span class="flag deprecated">deprecated</span>
                                {{/if}}
                            </li>
                        {{/methods}}
                    </ul>
                </div>
            {{/if}}

            {{#if properties}}
                <div class="index-section properties">
                    <h3>成员</h3>
                    <br/>

                    <table>
                    <tr><th>描述</th><th>成员</th></tr>
                    {{#properties}}
                    <tr><td>{{brief}}</td><td><a href="#property_{{name}}">{{name}}</a></td></tr>
                    {{/properties}}              
                    </table>


                    <ul class="index-list properties{{#if extends}} extends{{/if}}">
                        {{#properties}}
                            <li class="index-item property{{#if access}} {{access}}{{/if}}{{#if deprecated}} deprecated{{/if}}{{#if overwritten_from}} inherited{{/if}}{{#if extended_from}} inherited{{/if}}">
                               
                                {{#if static}}
                                    <span class="flag static">static</span>
                                {{/if}}
                                {{#if deprecated}}
                                    <span class="flag deprecated">deprecated</span>
                                {{/if}}
                            </li>
                        {{/properties}}
                    </ul>
                </div>
            {{/if}}

            {{#if attrs}}
                <div class="index-section attrs">
                    <h3>属性</h3>

                    <ul class="index-list attrs{{#if extends}} extends{{/if}}">
                        {{#attrs}}
                            <li class="index-item attr{{#if access}} {{access}}{{/if}}{{#if deprecated}} deprecated{{/if}}{{#if overwritten_from}} inherited{{/if}}{{#if extended_from}} inherited{{/if}}">
                                <a href="#attr_{{name}}">{{name}}</a>
                            </li>
                        {{/attrs}}
                    </ul>
                </div>
            {{/if}}

            {{#if events}}
                <div class="index-section events">
                    <h3>事件</h3>

                    <ul class="index-list events{{#if extends}} extends{{/if}}">
                        {{#events}}
                            <li class="index-item event{{#if access}} {{access}}{{/if}}{{#if deprecated}} deprecated{{/if}}{{#if overwritten_from}} inherited{{/if}}{{#if extended_from}} inherited{{/if}}">
                                <a href="#event_{{name}}">{{name}}</a>

                                {{#if static}}
                                    <span class="flag static">static</span>
                                {{/if}}
                                {{#if deprecated}}
                                    <span class="flag deprecated">deprecated</span>
                                {{/if}}
                            </li>
                        {{/events}}
                    </ul>
                </div>
            {{/if}}
        </div>

        {{#if methods}}
            <div id="methods" class="api-class-tabpanel">
                <h2 class="off-left">方法</h2>

                {{#methods}}
                    {{>method}}
                {{/methods}}
            </div>
        {{/if}}

        {{#if properties}}
            <div id="properties" class="api-class-tabpanel">
                <h2 class="off-left">成员变量</h2>

                {{#properties}}
                    {{>props}}
                {{/properties}}
            </div>
        {{/if}}

        {{#if attrs}}
            <div id="attrs" class="api-class-tabpanel">
                <h2 class="off-left">属性</h2>

                {{#attrs}}
                    {{>attrs}}
                {{/attrs}}
            </div>
        {{/if}}

        {{#if events}}
            <div id="events" class="api-class-tabpanel">
                <h2 class="off-left">事件</h2>

                {{#events}}
                    {{>events}}
                {{/events}}
            </div>
        {{/if}}
    </div>
</div>
